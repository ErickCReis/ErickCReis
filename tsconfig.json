{
  "extends": "astro/tsconfigs/strict",
  "include": ["alchemy.run.ts", ".astro/types.d.ts", "src/**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "types": ["@cloudflare/workers-types", "astro/client", "./src/env.d.ts"]
  }
}
