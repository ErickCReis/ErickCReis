{
  "name": "erickcreis",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development bun --hot src/server/index.ts",
    "content:sync": "bun run ./content-sync.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "lint": "bun run content:sync && oxlint --fix && oxfmt && bunx tsc --noEmit",
    "build": "bun run content:sync && bun run ./build.ts",
    "start": "cd ./dist && NODE_ENV=production ./server"
  },
  "dependencies": {
    "@elysiajs/eden": "^1.4.8",
    "@elysiajs/static": "^1.4.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "elysia": "^1.4.25",
    "fuma-content": "^1.1.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "2.15.4",
    "tailwind-merge": "^3.5.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bun": "^1.3.9",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "bun-plugin-tailwind": "^0.1.2",
    "oxfmt": "^0.35.0",
    "oxlint": "^1.50.0",
    "tailwindcss": "^4.2.1",
    "typescript": "^5.9.3"
  }
}
