{
  "name": "erickcreis",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "bun run --parallel dev:astro dev:api",
    "dev:astro": "bun --bun astro dev --host",
    "dev:api": "bun --watch src/server/index.ts",
    "lint": "oxlint --fix && oxfmt && tsc --noEmit && astro check",
    "build": "bun --bun astro build",
    "start": "NODE_ENV=production bun src/server/index.ts"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.13",
    "@astrojs/solid-js": "^5.1.3",
    "@elysiajs/cors": "^1.4.1",
    "@elysiajs/eden": "^1.4.8",
    "@solid-primitives/mouse": "^2.1.6",
    "@solid-primitives/scheduled": "^1.5.3",
    "@solid-primitives/tween": "^1.4.1",
    "astro": "5.18.0",
    "clsx": "^2.1.1",
    "elysia": "^1.4.26",
    "solid-js": "^1.9.11",
    "tailwind-merge": "^3.5.0",
    "valibot": "^1.1.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "@tailwindcss/vite": "^4.2.1",
    "@types/bun": "^1.3.9",
    "oxfmt": "^0.35.0",
    "oxlint": "^1.50.0",
    "tailwindcss": "^4.2.1",
    "typescript": "^5.9.3"
  }
}
